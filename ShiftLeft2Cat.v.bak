module ShiftLeft2 
(   
	input [25:0]  DataInput,
	input [3:0]	  Concatenate,
   output reg [31:0] DataOutput

);
   always @ (DataInput)
     DataOutput = {{Concatenate[3:0]}, {DataInput[25:0], 1'b0, 1'b0}};

endmodule // leftShift2